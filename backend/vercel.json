{
    "version": 2,
    "builds": [
        {
            "src": "/(.*)",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "api/*.js",
            "dest": "backend/index.js"
        },
        {
            "src": "api/*.js",
            "dest": "backend/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "backend/index.js"
        }
    ]
}
