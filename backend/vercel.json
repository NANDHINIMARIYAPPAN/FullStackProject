{
    "version": 2,
    "builds": [
        {
            "src": "backend/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/getcontactUs",
            "dest": "backend/index.js"
        },
        {
            "src": "/api/contactUs",
            "dest": "backend/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "backend/index.js"
        }
    ]
}
